<header>
    <%- include('./partials/header'); %>
</header><br><br>
<div class="container">  
    <div class="jumbotron">     
    <form action="/account" method="post" style="font-family: monospace;">
        <div class="form-group">
          <label>Phone Number</label>
          <input type="text" class="form-control" name="number" placeholder="Enter Phone Number">          
        </div>
        <div class="form-group">
          <label>Password</label>
          <input type="password" class="form-control" name="password" placeholder="Password">
        </div>
        <button type="submit" class="btn btn-primary">Login</button>
      </form>  
      <p class="display-3"><a href="/account">Login</a> or <a href="/register">Sign Up</a></p>         
    </div>                     
</div>   
    
<footer>
    <%- include('./partials/footer'); %>
</footer>